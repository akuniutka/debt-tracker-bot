<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm" version="3.0">
    <entity class="dev.akuniutka.chatbot.core.Chat" name="Chat">
        <attributes>
            <id name="userId" access="FIELD">
                <column name="user_id" />
            </id>
            <basic name="state">
                <column name="chat_state" nullable="false" />
            </basic>
        </attributes>
    </entity>
</entity-mappings>